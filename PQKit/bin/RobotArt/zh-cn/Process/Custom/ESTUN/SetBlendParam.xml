<?xml version="1.0" encoding="utf-8"?>
<ProcessParameter>
  <Title>设置过渡参数</Title>
  <GroupColl>
      <Group>
	<ProcessInfoColl>
	<ProcessInfo>
		<Name>类型</Name>
		<Id>[[6]]</Id>
		<Type>Comb</Type>
		<Unit></Unit>
		<defaultValue>过渡类型</defaultValue>
		<AllValue>
			<Value>
				<ShowValue>FINE</ShowValue>
				<OutValue>FINE</OutValue>
			</Value>
			<Value>
				<ShowValue>RELATIVE</ShowValue>
				<OutValue>RELATIVE</OutValue>
			</Value>
			<Value>
				<ShowValue>ABSOLUTE</ShowValue>
				<OutValue>ABSOLUTE</OutValue>
			</Value>
		</AllValue>
    </ProcessInfo>
    <ProcessInfo>
		<Name>FINE</Name>
		<Id>[[1]]</Id>
		<Type>Edit</Type>
		<Unit>FINE</Unit>
		<defaultValue>FINE</defaultValue>
		<Dependedid>[[6]]</Dependedid>
		<DependedAllValue>
			<DependedValue>FINE</DependedValue>
		</DependedAllValue>
    </ProcessInfo>
	<ProcessInfo>
		<Name>RELATIVE</Name>
		<Id>[[2]]</Id>
		<Type>Edit</Type>
		<Unit>0-1000</Unit>
		<defaultValue>0</defaultValue>
		<min>0</min>
		<max>1000</max>
		<Dependedid>[[6]]</Dependedid>
		<DependedAllValue>
			<DependedValue>RELATIVE</DependedValue>
		</DependedAllValue>
    </ProcessInfo>
	<ProcessInfo>
		<Name>ABSOLUTE</Name>
		<Id>[[3]]</Id>
		<Type>Edit</Type>
		<Unit>0-1000</Unit>
		<defaultValue>0</defaultValue>
		<min>0</min>
		<max>1000</max>
		<Dependedid>[[6]]</Dependedid>
		<DependedAllValue>
			<DependedValue>ABSOLUTE</DependedValue>
		</DependedAllValue>
    </ProcessInfo>
  </ProcessInfoColl>
      </Group>
  </GroupColl>
  <statementSrcColl>
	<statement>
		<Part>SetBlendParam("[[1]]")</Part>
		<Dependedid>[[6]]</Dependedid>
		<DependedValue>FINE</DependedValue>
	</statement>
	<statement>
		<Part>SetBlendParam("RELATIVE",l:C[[2]])</Part>
		<Dependedid>[[6]]</Dependedid>
		<DependedValue>RELATIVE</DependedValue>
	</statement>
	<statement>
		<Part>SetBlendParam("ABSOLUTE",l:C[[3]])</Part>
		<Dependedid>[[6]]</Dependedid>
		<DependedValue>ABSOLUTE</DependedValue>
	</statement>
  </statementSrcColl>
  <statementDatColl>
     <statement>
		<Part>C[[2]]={_type="ZONE",per=[[2]],dis=[[2]],vConst=0}</Part>
		<Dependedid>[[6]]</Dependedid>
		<DependedValue>RELATIVE</DependedValue>
	</statement>
	<statement>
		<Part>C[[3]]={_type="ZONE",per=[[3]],dis=[[3]],vConst=0}</Part>
		<Dependedid>[[6]]</Dependedid>
		<DependedValue>ABSOLUTE</DependedValue>
	</statement>
  </statementDatColl>
</ProcessParameter>

